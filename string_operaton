#include <stdio.h>
#include<string.h>
#include<ctype.h>
int main()
{
	char s[100];
	scanf("%s",s);
	int len=strlen(s);
	int i=0,j=len-1;
	while(i<j)
	{
		if(!isalpha(s[i])) {
			i++;
		}
		else if(!isalpha(s[j])) {
			j--;
		}
		else {
			char t=s[i];
			s[i]=s[j];
			s[j]=t;
			i++;
			j--;
		}
	}
	printf("%s\n",s);
	return 0;
}
